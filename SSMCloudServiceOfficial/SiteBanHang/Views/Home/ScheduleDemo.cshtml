    <script>
        function savetheday(hour, minute, btnID, dayofweek) {
            $(".mybtn").each(function () {
                $(this).attr('class', 'mybtn btn btn-primary');
            })
            $("#" + btnID).attr('class', 'mybtn btn btn-success');
            var thisweekmonday = Date.parseExact($("#fromdate").val(), "MM/dd/yyyy");
            var choosedDate = thisweekmonday.add({ days: (dayofweek - 1) });
            var anothereven = new Object();
            var start = new Date(thisweekmonday.getFullYear(), thisweekmonday.getMonth(), thisweekmonday.getDate(), hour, minute, 0, 0);
            anothereven.start = new Date(thisweekmonday.getFullYear(), thisweekmonday.getMonth(), thisweekmonday.getDate(), hour, minute, 0, 0);
            anothereven.end = start.add({ minutes: 15 });
            anothereven.allDay = false;
            anothereven.title = "clientchoose";
            anothereven.id = "myEvent";
            chosenevent = anothereven;
            choosenDate = [];
            choosenDate.push(anothereven);
            alert(JSON.stringify(choosenDate));
        }
    </script>
    @if (ViewData["unavailabledate"] != null)
    {
        foreach (Calendar cal in (List<Calendar>)ViewData["unavailabledate"])
        {
            <script>

                var myEvent@(cal.id) = new Object();
                myEvent@(cal.id).title = "Unavailable";
                myEvent@(cal.id).start = new Date('@cal.startTime');
                myEvent@(cal.id).end = new Date('@cal.endTime');
                myEvent@(cal.id).dow = [@((int)cal.startTime.DayOfWeek)];
                myEvent@(cal.id).backgroundColor = '#545454',
                myEvent@(cal.id).editable = false,

                globalEvent.push(myEvent@(cal.id));
            </script>
        }
    }

