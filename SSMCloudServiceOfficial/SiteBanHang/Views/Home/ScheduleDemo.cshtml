    <script>
        function nextweek() {
            var thisweekmonday = Date.parseExact($("#fromdate").val(), "MM/dd/yyyy");
            var thisweeksaturday = Date.parseExact($("#todate").val(), "MM/dd/yyyy");
            thisweekmonday = thisweekmonday.add({ days: 7 });
            thisweeksaturday = thisweeksaturday.add({ days: 7 });

            $("#fromdate").val(thisweekmonday.toString("MM/dd/yyyy"));
            $("#todate").val(thisweeksaturday.toString("MM/dd/yyyy"));
        }

        function previousweek() {
            var thisweekmonday = Date.parseExact($("#fromdate").val(), "MM/dd/yyyy");
            var thisweeksaturday = Date.parseExact($("#todate").val(), "MM/dd/yyyy");
            thisweekmonday = thisweekmonday.add({ days: -7 });
            thisweeksaturday = thisweeksaturday.add({ days: -7 });

            $("#fromdate").val(thisweekmonday.toString("MM/dd/yyyy"));
            $("#todate").val(thisweeksaturday.toString("MM/dd/yyyy"));
        }
    </script>


