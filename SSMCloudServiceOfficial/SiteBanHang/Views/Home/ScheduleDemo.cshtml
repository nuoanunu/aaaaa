                slotDuration: '00:15:00',
                allDaySlot: false,
                contentHeight: 600,
                defaultView: 'agendaWeek',
                header: {
                    left: 'prev,next today',
                    center: 'title',

                },
                selectable: true,
                selectHelper: true,
                select: function (start, end, allDay) {
                    $('#fc_create').click();

                    started = start;
                    ended = end;
                    startedChoosed = new Date(start);



                },
                eventClick: function (calEvent, jsEvent, view) {

                    var eid = calEvent.id;

                    if (calEvent.title != "Unavailable") {
                        $('#fc_delete').click();

                        categoryClass = $("#event_type").val();

                        $(".Delete").on("click", function () {
                            calendar.fullCalendar('removeEvents', eid);

                            $('.antoclose2').click();
                        });
                        calendar.fullCalendar('unselect');
                    }

                },
                editable: true,
                events: globalEvent
            });


            $(".antosubmit").on("click", function () {
                calendar.fullCalendar('removeEvents', "myEvent");

                var datestart = startedChoosed.add({ hours: -7 });
                var dateend = new Date(startedChoosed);
                dateend = dateend.addMinutes(15);

                anothereven = new Object();
                alert(datestart);
                alert(dateend);
                anothereven.start = datestart;
                anothereven.end = dateend;
                anothereven.allDay = false;
                anothereven.title = "clientchoose";
                anothereven.id = "myEvent";
                choosenDate = anothereven;



                categoryClass = $("#event_type").val();

                if ("clientchoose") {
                    $("#calendar").fullCalendar('renderEvent', anothereven,
                      true // make the event "stick"
                    );
                }
                $("#calendar").fullCalendar('unselect');
                $('.antoclose').click();

                return false;
            });
        });

    </script>




