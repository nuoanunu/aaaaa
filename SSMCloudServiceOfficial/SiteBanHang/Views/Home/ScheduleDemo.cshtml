                                        Next
                                    </button>

                                    <br />
                                    <input disabled type="text" id="fromdate" value="@DateTime.Now.AddDays( 1-(int)DateTime.Now.DayOfWeek).ToShortDateString()" />
                                    <input disabled type="text" id="todate" value="@DateTime.Now.AddDays( 5-(int)DateTime.Now.DayOfWeek).ToShortDateString()" />
                                </div>

                                </br>
                                <div class="pre-scrollable" style="align-content:center;max-height:600px">
                                    <table border='1' cellpadding='0' cellspacing='0' id="myscheduler" width="100%">
                                        <tr class='days'>

                                            <th>Monday</th>
                                            <th>Tuesday</th>
                                            <th>Wednesday</th>
                                            <th>Thursday</th>
                                            <th>Friday</th>

                                        </tr>
                                        @for (int i = 0; i < 36; i++)
                                        {
                                            TimeSpan meetingstart = new TimeSpan(0, (int)(8 + Math.Floor((double)i / 4)), (((int)(double)i % 4) * 15), 0);
                                            TimeSpan meetingend = meetingstart.Add(new TimeSpan(0, 0, 15, 0));

                                            <tr>
                                                @for (int j = 1; j < 6; j++)
                                                {
                                                    String hours = (8 + Math.Floor((double)i / 4)) + ""; if (hours.Length == 1) { hours = "0" + hours; }
                                                    String minutes = (((int)(double)i % 4) * 15) + ""; if (minutes.Length == 1) { minutes = "0" + minutes; }

                                                    if (ViewData["unavailabledate"] == null)
                                                    {
                                                        <td>
                                                            <button id="@(i * 6 + j)" onclick="savetheday(@hours, @minutes,@(i * 6 + j) , @j )" class="mybtn btn btn-primary" style="width:100px;margin:3px 3px 3px 3px">@hours:@minutes</button>
                                                        </td>
                                                    }

                                                    else
                                                    {
                                                        bool avail = true;
                                                        foreach (Calendar cal in (List<Calendar>)ViewData["unavailabledate"])
                                                        {

                                                            if ((int)cal.startTime.DayOfWeek == j)
                                                            {
                                                                if (cal.startTime.TimeOfDay < meetingend && cal.endTime.TimeOfDay > meetingstart)
                                                                {
                                                                    avail = false;
                                                                }

                                                            }

                                                        }
                                                        if (avail)
                                                        {
                                                            <td>
                                                            <button id = "@(i * 6 + j)" onclick = "savetheday(@hours, @minutes,@(i * 6 + j) , @j )" class="mybtn btn btn-primary" style="width:100px;margin:3px 3px 3px 3px">@hours:@minutes</button>
                                                            </td>
                                                        }
                                                        else
                                                        {
                                                            <td style="background-color:slategray" align="center">
                                                                @hours:@minutes
                                                            </td>
                                                        }
                                                    }
                                                }
                                            </tr>

                                        }
                                    </table>
                                </div>
                            </div>
                        </div>
